            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-sm-3">Contact Number:</label>
            <div class="col-sm-8">
                <input type="text"  pattern="[0-9]{10}" name="contact" placeholder="Enter Contact Number" class="form-control" maxlength="10" required />
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-sm-3">R Contacts:</label>
            <div class="col-sm-8">
               <input type="text" name="rcontact1" pattern="[0-9]{10}" placeholder="Enter Relatives Contact 1st" class="form-control" maxlength="10" required>
             <input type="text" name="rcontact2" pattern="[0-9]{10}" placeholder="Enter Relatives Contact 2nd" class="form-control" maxlength="10" required>
             <input type="text" name="rcontact3" pattern="[0-9]{10}" placeholder="Enter Relatives Contact 3" class="form-control"  maxlength="10" required>
             <input type="text" name="rcontact4" pattern="[0-9]{10}" placeholder="Enter Relatives Contact 4" class="form-control" maxlength="10" required>
          
            </div>
        </div>
        
        <div class="form-group">
            <label class="control-label col-sm-3">Person Pic:</label>
            <div class="col-sm-8">
                <input type="file" name="pimg"  class="form-control" required>
            </div>
                  
        </div>

        @Html.Captcha(4)

        <p class="error">
            
<font color="green">
<div style="padding:20px;"> 
      @if (ViewBag.ErrMessage != null)
         { 
    
     @ViewBag.ErrMessage

    }
    </div>
    </font>  


        
             <input type="submit" class="btn btn-danger" style="margin-left:150px; margin-top:10px;"></div>



